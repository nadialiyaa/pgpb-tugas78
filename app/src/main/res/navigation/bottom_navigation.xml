<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/bottom_navigation"
    app:startDestination="@id/navigation_ticket">

    <fragment
        android:id="@+id/navigation_ticket"
        android:name="com.example.penugasan7.ticketFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_ticket" >
        <action
            android:id="@+id/action_navigation_ticket_to_navigation_choose_type_ticket"
            app:destination="@id/navigation_choose_type_ticket" />
    </fragment>
    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.penugasan7.homeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_navigation_home_to_navigation_list"
            app:destination="@id/navigation_list" />
    </fragment>
    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.penugasan7.profileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />

    <fragment
        android:id="@+id/navigation_choose_type_ticket"
        android:name="com.example.penugasan7.chooseFragment"
        android:label="Choose Ticket Type" />

    <fragment
        android:id="@+id/navigation_list"
        android:name="com.example.penugasan7.listFragment"
        android:label="Ticket List" />
    <fragment
        android:id="@+id/orderticketFragment"
        android:name="com.example.penugasan7.orderticketFragment"
        android:label="orderticketFragment" />


</navigation>